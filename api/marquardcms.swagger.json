        "/oauth2/token": {
            "post": {
                "summary": "Get a token for user/device with ouath2.\n",
                "parameters": [
                    {
                        "name": "client_id",
                        "in": "query",
                        "required": true,
                        "type": "string"
                    },
                    {
                        "name": "client_secret",
                        "in": "query",
                        "required": true,
                        "type": "string"
                    },
                    {
                        "name": "grant_type",
                        "in": "query",
                        "required": true,
                        "type": "string",
                        "description": "'client_credentials' or 'password'\n"
                    },
                    {
                        "name": "username",
                        "in": "query",
                        "required": false,
                        "type": "string"
                    },
                    {
                        "name": "password",
                        "in": "query",
                        "required": false,
                        "type": "string"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OAuth2 appropriate standard answer"
                    }
                }
            }
        },
        "/oauth2/token-with-facebook": {
            "post": {
                "summary": "Login with an exist user, or create new account.\n",
                "parameters": [
                    {
                        "name": "client_id",
                        "in": "query",
                        "required": true,
                        "type": "string"
                    },
                    {
                        "name": "client_secret",
                        "in": "query",
                        "required": true,
                        "type": "string"
                    },
                    {
                        "name": "token",
                        "in": "query",
                        "required": true,
                        "type": "string",
                        "description": "Facebook token\n"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OAuth2 appropriate standard answer"
                    },
                    "default": {
                        "description": "Unexpected error",
                        "schema": {
                            "$ref": "#/definitions/Error"
                        }
                    }
                }
            }
        },
